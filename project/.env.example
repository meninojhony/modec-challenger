# Exemplo de variáveis de ambiente para produção
# Copie este arquivo para .env e configure os valores

# Database
DATABASE_URL=sqlite:///./contracts.db

# API Configuration
DEBUG=False
PROJECT_NAME=Contract Management API
API_V1_STR=/api/v1

# Frontend URL (será fornecida pelo Railway)
FRONTEND_URL=https://your-app.railway.app

# CORS Origins (adicione o domínio do frontend)
CORS_ORIGINS=["https://your-frontend.railway.app", "https://your-app.railway.app"]